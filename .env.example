# Yeast Agent Configuration
# Copy this file to .env and update with your Apollo details

# Apollo host (can be hostname or IP)
APOLLO_HOST=apollo.local

# Apollo SSH username
APOLLO_USER=jay

# Apollo SSH port
APOLLO_PORT=22

# Path to yeast-agent on apollo
AGENT_PATH=~/yeast-agent

# Ollama / RAG Settings
# If Ollama is remote and not exposed, use EMBEDDING_VIA_SSH=true
# If you have a local Ollama or tunnel, you can set OLLAMA_API_URL=http://localhost:11434
EMBEDDING_VIA_SSH=true
OLLAMA_API_URL=
